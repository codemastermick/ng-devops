{
  "name": "ng-devops",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "git fetch && git pull",
    "start": "ng serve",
    "poststart": "npm run lint",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build-test": "npm run build && npm run test:ci",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "coverage": "cat coverage/ng-devops/lcov.info | codacy-coverage --username codemastermick --projectName ng-devops",
    "lint": "ng lint --fix",
    "lintonly": "ng lint",
    "e2e": "ng e2e",
    "version": "auto-changelog -p && git add -f CHANGELOG.md",
    "commit": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.1",
    "@angular/common": "~8.2.1",
    "@angular/compiler": "~8.2.1",
    "@angular/core": "~8.2.1",
    "@angular/forms": "~8.2.1",
    "@angular/platform-browser": "~8.2.1",
    "@angular/platform-browser-dynamic": "~8.2.1",
    "@angular/router": "~8.2.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.1",
    "@angular/language-service": "~8.2.1",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.1",
    "auto-changelog": "^1.14.1",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^5.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
